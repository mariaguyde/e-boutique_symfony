{% extends 'base.html.twig' %}

{% block title %}CommandLine{% endblock %}

{% block body %}
<div class="container">
    <h1>CommandLine</h1>
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ command_line.id }}</td>
            </tr>
            <tr>
                <th>Quantite</th>
                <td>{{ command_line.quantite }}</td>
            </tr>
        </tbody>
    </table>
    <a href="{{ path('command_line_index') }}">back to list</a>
    <a href="{{ path('command_line_edit', {'id': command_line.id}) }}">edit</a>
    {{ include('command_line/_delete_form.html.twig') }}
</div>
{% endblock %}
